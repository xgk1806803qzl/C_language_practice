p1000

#include<cstdio>
using namespace std;
int main()
{
   printf("                ********\n");
   printf("               ************\n");
   printf("               ####....#.\n");
   printf("             #..###.....##....\n");
   printf("             ###.......######              ###            ###\n");
   printf("                ...........               #...#          #...#\n");
   printf("               ##*#######                 #.#.#          #.#.#\n");
   printf("            ####*******######             #.#.#          #.#.#\n");
   printf("           ...#***.****.*###....          #...#          #...#\n");
   printf("           ....**********##.....           ###            ###\n");
   printf("           ....****    *****....\n");
   printf("             ####        ####\n");
   printf("           ######        ######\n");
   printf("##############################################################\n");
   printf("#...#......#.##...#......#.##...#......#.##------------------#\n");
   printf("###########################################------------------#\n");
   printf("#..#....#....##..#....#....##..#....#....#####################\n");
   printf("##########################################    #----------#\n");
   printf("#.....#......##.....#......##.....#......#    #----------#\n");
   printf("##########################################    #----------#\n");
   printf("#.#..#....#..##.#..#....#..##.#..#....#..#    #----------#\n");
   printf("##########################################    ############\n");
   return 0;
}

p1001
#include <cstdio>
using namespace std;

int main()
{
    int a, b;
    scanf("%d%d", &a, &b);
    printf("%d", a+b);
    return 0;
}

