192 384 576

p1035
#include<cstdio>
int main() {
    int k,n=0;
    scanf("%d",&k);
    for(double Sn=0;Sn<=k;++n,Sn+=1.0/n);
    printf("%d",n);
    return 0;
}

p1046

#include <cstdio>

int main() {
    int a[10], x, sum = 0;
    for (int i = 0; i < 10; i++)
        scanf("%d", &a[i]);
    scanf("%d", &x);
    x += 30;
    for (int i = 0; i < 10; i++)
        if (a[i] <= x) sum++;
    printf("%d", sum);
    return 0;
}
